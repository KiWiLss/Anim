<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.magicsoft.anim">

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.ToastActivity"/>
        <activity android:name=".activity.ToastActivity2"/>

        <activity android:name=".activity.FrameActivity"/>
        <activity android:name=".activity.ComplementActivity"/>
        <activity android:name=".activity.AttriButeActivity"/>
        <activity android:name=".activity.DialogActivity"/>
    </application>

</manifest>